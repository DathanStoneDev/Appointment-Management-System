<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wgu.stone.controller.LoginController">
   <children>
      <Button fx:id="loginButton" layoutX="217.0" layoutY="277.0" mnemonicParsing="false" onAction="#loginToApp" prefHeight="25.0" prefWidth="140.0" text="Login" />
      <Label fx:id="usernameLabel" layoutX="67.0" layoutY="163.0" text="User Name" />
      <Label fx:id="passwordLabel" layoutX="67.0" layoutY="217.0" prefHeight="17.0" prefWidth="72.0" text="Password" />
      <PasswordField fx:id="passwordField" layoutX="159.0" layoutY="213.0" prefHeight="25.0" prefWidth="256.0" />
      <TextField fx:id="userNameField" layoutX="159.0" layoutY="159.0" prefHeight="25.0" prefWidth="256.0" />
      <Label fx:id="titleLabel" layoutX="189.0" layoutY="64.0" text="Scheduling Application">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="locationLabel" layoutX="14.0" layoutY="369.0" prefHeight="17.0" prefWidth="106.0">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Label fx:id="loginErrorLabel" alignment="CENTER" layoutX="159.0" layoutY="245.0" prefHeight="17.0" prefWidth="256.0" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="System Bold Italic" size="10.0" />
         </font>
      </Label>
      <Button fx:id="exitAppButton" layoutX="562.0" layoutY="14.0" mnemonicParsing="false" onAction="#exitApplication" text="X" />
   </children>
</AnchorPane>
